{
  "name": "sharapov/php-ffmpeg-extensions",
  "description": "An extensions library for the PHP FFMpeg (https://github.com/PHP-FFMpeg/PHP-FFMpeg)",
  "license": "GPL-3.0",
  "type": "library",
  "keywords": [
    "ffmpeg",
    "php-ffmpeg",
    "ffmpeg overlay",
    "chromakey",
    "colorkey",
    "alpha layer",
    "mov",
    "video processing",
    "video",
    "audio processing",
    "audio",
    "avconv",
    "avprobe",
    "ffprobe"
  ],
  "homepage": "https://github.com/sharapovweb/php-ffmpeg-extensions",
  "authors": [
    {
      "name": "Alexander Sharapov",
      "email": "alexander@sharapov.biz",
      "homepage": "http://sharapov.biz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4 || ^7.0",
    "php-ffmpeg/php-ffmpeg": "^v0.18.0"
  },
  "require-dev": {
    "monolog/monolog": "^2.0"
  },
  "suggest": {
    "php-ffmpeg/extras": "A compilation of common audio & video drivers for PHP-FFMpeg"
  },
  "autoload": {
    "psr-4": {
      "Sharapov\\FFMpegExtensions\\": "src/php-ffmpeg-extensions/"
    }
  }
}
